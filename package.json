  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "lint": "nx lint",
    "reset": "rm -rf node_modules && rm -f package-lock.json && npm i",
    "test": "nx test",
    "test:watch": "nx test --watch",
    "e2e": " nx run e2e:e2e",
    "e2e:gui": " nx run e2e:e2e --watch",
    "build:serve": "nx build && serve dist/ng-performance -s",
    "serve": "serve dist/ng-performance -s",
    "caddy": "caddy start",
    "caddy:stop": "caddy stop",
    "build:wba": "nx b --stats-json && webpack-bundle-analyzer dist/ng-performance/stats.json",
    "wba": "webpack-bundle-analyzer dist/ng-performance/stats.json",
    "build:sme": "nx b --source-map && source-map-explorer dist/ng-performance/**/*.js",
    "sme": "source-map-explorer dist/ng-performance/**/*.js",
    "igv": "npx @rx-angular/import-graph-visualizer --entry-points src/main.ts --ts-config tsconfig.app.json",
    "dpdm": "npx dpdm --no-warning --no-tree src/main.ts"
  },
